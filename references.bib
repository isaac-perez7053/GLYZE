	month = dec,
	year = {2000},
	pages = {1255--1262},
	file = {viscocity_predictions_for_fatty_systems.pdf:files/948/viscocity_predictions_for_fatty_systems.pdf:application/pdf},
}

@article{cordina_cogito_2023,
	title = {{COGITO}: {A} {Coarse}-{Grained} {Force} {Field} for the {Simulation} of {Macroscopic} {Properties} of {Triacylglycerides}},
	volume = {19},
	copyright = {https://creativecommons.org/licenses/by/4.0/},
	issn = {1549-9618, 1549-9626},
	shorttitle = {{COGITO}},
	url = {https://pubs.acs.org/doi/10.1021/acs.jctc.2c00975},
	doi = {10.1021/acs.jctc.2c00975},
	abstract = {The use of molecular dynamics simulations is becoming ever more widespread; however, the application of this to pure triacylglyceride (TAG) systems is not. In this study, we are presenting the development, and validation, of a new force field (FF), which we have called the COarse-Grained Interchangeable Triacylglyceride-Optimized FF. The FF has been developed using both a bottom-up and top-down approach for different parameters, with the non-bonded parameters being optimized using a Bayesian optimization method. While the FF was developed using monounsaturated TAGs, results show that it is also suitable for fully saturated TAGs. Description of molecules which were not used during the development of the FF is carried out simply by interchanging the bead in the molecule topologies. Results show that the FF can reproduce the macroscopic properties (density and lattice parameters) of pure TAGs as both crystals and melt with high accuracy, as well as reproduce the differences in enthalpies.},
	language = {en},
	number = {4},
	urldate = {2025-09-19},
	journal = {J. Chem. Theory Comput.},
	author = {Cordina, Robert J. and Smith, Beccy and Tuttle, Tell},
	month = feb,
	year = {2023},
	pages = {1333--1341},
	file = {cordina-et-al-2023-cogito-a-coarse-grained-force-field-for-the-simulation-of-macroscopic-properties-of-triacylglycerides.pdf:files/1006/cordina-et-al-2023-cogito-a-coarse-grained-force-field-for-the-simulation-of-macroscopic-properties-of-triacylglycerides.pdf:application/pdf},
}
